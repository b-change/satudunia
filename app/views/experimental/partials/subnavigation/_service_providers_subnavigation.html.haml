-	get_country_name(@service_providers).each do |provider|
	%li
		/ = link_to "#{provider}",country_services_map_path(provider.downcase)
		-if provider == "Kuala"
			= link_to "Kuala Lumpur",country_services_map_path("Kuala-Lumpur")
		- else
			= link_to "#{provider}",country_services_map_path(provider.downcase)